;; Guix channel definition for rust-xous
;;
;; Add this channel to your ~/.config/guix/channels.scm:
;;
;;   (cons (channel
;;           (name 'rust-xous)
;;           (url "https://github.com/sbellem/guix-rustup"))
;;         %default-channels)
;;
;; Then run: guix pull
;; And build: guix build rust-xous

(channel
  (version 0)
  (directory ".")
  (dependencies
   (channel
    (name rustup)
    (url "https://github.com/sbellem/guix-rustup")
    (branch "dev")
    (introduction
     (channel-introduction
      (version 0)
      (commit "d9bcf7f979506b880a5ba25674a606a824d9c890")
      (signer "E39D 2B3D 0564 BA43 7BD9  2756 C38A E0EC CAB7 D5C8"))))
   (channel
    (name nonguix)
    (url "https://gitlab.com/nonguix/nonguix")
    (introduction
     (channel-introduction
      (version 0)
      (commit "897c1a470da759236cc11798f4e0a5f7d4d59fbc")
      (signer "2A39 3FFF 68F4 EF7A 3D29  12AF 6F51 20A0 22FB B2D5"))))))
